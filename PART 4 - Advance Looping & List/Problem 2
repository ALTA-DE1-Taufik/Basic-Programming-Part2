def draw_xyz(N):
    # your code here
    current = 1
    for i in range(1, N + 1):
        for j in range(1, N + 1):
            if current % 3 == 0:
                print("X", end=" ")
            elif current % 2 == 0:
                print("Z", end=" ")
            else:
                print("Y", end=" ")
            current += 1
        print()

print(draw_xyz(3))

print(draw_xyz(5))